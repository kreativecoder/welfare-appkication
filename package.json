{
  "name": "welfare-application",
  "version": "1.0.0",
  "description": "welfare application",
  "main": "server/app.js",
  "scripts": {
    "build-server": "babel server --out-dir dist",
    "build": "npm run clean && npm run build-server",
    "seed": "npx sequelize-cli db:seed:all",
    "migration": "npx sequelize-cli db:migrate",
    "start": "node dist/app.js",
    "dev": "nodemon --exec babel-node server/app.js",
    "heroku-postbuild": "npm run build && npm run migration && npm run seed",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "cooporative",
    "coporative",
    "welfare",
    "coporate"
  ],
  "author": "Adeleye Jeremiah",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jerrycaffe/cooporative-app/tree/master/backend"
  },
  "bugs": {
    "url": "https://github.com/jerrycaffe/cooporative-app/issues"
  },
  "homepage": "https://github.com/jerrycaffe/cooporative-app/tree/master/backend",
  "license": "ISC",
  "dependencies": {
    "@babel/node": "^7.10.1",
    "babel-preset-env": "^1.7.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.22.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "multer-storage-cloudinary": "^4.0.0",
    "pg": "^8.2.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.13"
  },
  "engines": {
    "node": "12.15.0"
  },
