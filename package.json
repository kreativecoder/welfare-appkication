    "@babel/cli": "^7.13.0",
    "@babel/core": "^7.13.1",
    "@babel/preset-env": "^7.13.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "nodemon": "^2.0.4"
  }
